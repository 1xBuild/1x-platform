{
  "name": "discord-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "cross-env NODE_ENV=production ts-node app/backend/server.ts",
    "start:dev": "cross-env NODE_ENV=development ts-node app/backend/server.ts",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --watch src app/backend/server.ts",
    "build": "tsc",
    "build:dev": "NODE_ENV=development tsc",
    "build:prod": "NODE_ENV=production tsc",
    "build:front": "cd app/frontend && npm build",
    "build:back": "cd app/backend && npm build",
    "build:all": "npm run build:front && npm run build:back"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@letta-ai/letta-client": "^0.1.80",
    "better-sqlite3": "^11.9.1",
    "cors": "^2.8.5",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "openai": "^4.28.0",
    "pino": "^9.7.0",
    "telegraf": "^4.16.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
